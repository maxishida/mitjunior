{
  "name": "mitsuo-ishida-landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:production": "NODE_ENV=production npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "deploy": "firebase deploy --project mitjunior",
    "deploy:production": "npm run build:production && firebase deploy --only hosting --project mitjunior",
    "deploy:all": "npm run build:production && firebase deploy --project mitjunior",
    "deploy:rules": "firebase deploy --only firestore:rules,storage --project mitjunior",
    "serve": "firebase serve --port 3000",
    "emulator": "firebase emulators:start",
    "emulator:ui": "firebase emulators:start --ui",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.4",
    "jest": "^30.2.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  }
}
